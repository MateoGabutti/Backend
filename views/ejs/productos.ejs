<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <title>ProductosEjs</title>
</head>
<body> 
  <div class="container row justify-content-center">
      <% if (objetos) { %>
        <table class="table table-dark table-hover col-10">
          <thead>
            <tr>
              <th scope="col">Titulo</th>
              <th scope="col">Precio</th>
              <th scope="col">Imagen</th>
              <th scope="col">Id</th>
            </tr>
          </thead>
          <tbody>
              <% objetos.forEach(item => { %>
                  <tr>
                      <th scope="row"><%= item.title %> </th>
                      <td><%= item.price %> </td>
                      <td><%= item.thumbnail %> </td>
                      <td><%= item.id %> </td>
                  </tr>
              <% }) %>
          </tbody>
        </table>
      <% } else { %>      
          <h3 class="text-center alert alert-primary">No hay productos</h3>
      <% } %>
      <a href="http://localhost:3001" class="btn btn-dark col-3 align-self-end">Ir al formulario</a>
    </div>
</body>
</html>